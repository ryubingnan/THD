    <script>
        function entryChange1() {
            radio = document.getElementsByName('A6')
            if (radio[0].checked) {
                document.getElementById('js-a6_1').style.display = "table-row";
                document.getElementById('js-a6_3').style.display = "none";
            } else if (radio[1].checked) {
                document.getElementById('js-a6_1').style.display = "none";
                document.getElementById('js-a6_3').style.display = "none";
            } else if (radio[2].checked) {
                document.getElementById('js-a6_1').style.display = "none";
                document.getElementById('js-a6_3').style.display = "table-row";
            } else if (radio[3].checked) {
                document.getElementById('js-a6_1').style.display = "none";
                document.getElementById('js-a6_3').style.display = "none";
            }
        }

        window.onload = entryChange1;

        window.onload = function () {
            entryChange1;

            var f = document.getElementById("form1");
            checkSelect(f.elements["A3_1"], "");
            checkSelect(f.elements["A3_2"], "");
            checkSelect(f.elements["A3_3"], "");

            method_A1();
            method_A2_1();
            method_A2_2();
            method_A3();

            formClear();

        }

        function checkSelect(obj, val) {
            for (var i = 0; i < obj.length; i++) {
                if (obj[i].value == val) {
                    obj[i].selected = true;
                    break;
                }
            }
        }

        function checkSelectClear(obj) {
            for (var i = 0; i < obj.length; i++) {
                obj[i].selected = false;
            }
        }

        function getCookie(name) {
            var r = null;
            var c = name + '=';
            var allcookies = document.cookie;
            var position = allcookies.indexOf(c);
            if (position != -1) {
                var startIndex = position + c.length;
                var endIndex = allcookies.indexOf(';', startIndex);
                if (endIndex == -1) {
                    endIndex = allcookies.length;
                }
                r = decodeURIComponent(allcookies.substring(startIndex, endIndex));
            }
            return r;
        }

        function formClear() {
            if (getCookie('return') == '1') {
                document.getElementById("form1").Areply_mail.value = "";
                document.getElementById("form1").A1_1.value = "";
                document.getElementById("form1").A1_2.value = "";
                document.getElementById("form1").A2_1.value = "";
                document.getElementById("form1").A2_2.value = "";
                checkSelectClear(document.getElementById("form1").elements["A3_1"]);
                checkSelectClear(document.getElementById("form1").elements["A3_2"]);
                checkSelectClear(document.getElementById("form1").elements["A3_3"]);
                document.getElementById("form1").A4.value = "";
                document.getElementById("form1").A5.value = "";
                document.getElementById("form1").A6_1.checked = "";
                document.getElementById("form1").A6_2.checked = "";
                document.getElementById("form1").A6_3.checked = "";
                document.getElementById("form1").A6_4.checked = "";
                document.getElementById("form1").A7.value = "";
                document.getElementById("form1").A8.value = "";
                document.getElementById("form1").PRIVACY_CHECK.checked = false;
            }

        }
        function form1Submit() {
            if (!inputCheck()) {
                return
            }
            $('#form1').css('display', 'none');
            $('#form2').css('display', 'block');
            $('#progress1').removeClass('is-current');
            $('#progress2').addClass('is-current');
            formcheck_Confirm();
            $(document).scrollTop(0);
        }

        function form2Submit() {
            $('#form2').css('display', 'none');
            $('#form3').css('display', 'block');
            $('#progress2').removeClass('is-current');
            $('#progress3').addClass('is-current');
            $(document).scrollTop(0);
        }

        function form3Submit() {
            var data = {
                'ContentsofInquiry': $('input[name=A6]:checked').val(),
                'FirstName': $('#A1_1').val(),
                'LastName': $('#A1_2').val(),
                'Surname': $('#A2_1').val(),
                'Mei': $('#A2_2').val(),
                'BirthDate': $('#A3').val(),
                'Phone': $('#A4').val(),
                'Email': $('#Areply_mail').val(),
                'ErrorCode': $('#A7').val(),
                'ProposalNumber': $('#A8').val(),
                'Content': $('#A5').val()
            }
            $.ajax({
                url: '/Corporate/AddCorporate',
                type: 'post',
                dataType: 'json',
                data: JSON.stringify(data),
                contentType: 'application/json;charset=utf-8',
                success: function (res) {
                    debugger
                    if (res.success) {
                        alert("添加成功")
                        location.href = "/";
                    }
                }
            })
        }

        function formcheck_Confirm() {
            radio = document.getElementsByName('A6')
            if (radio[0].checked) {
                document.getElementById('form2A6_1').checked = true
                document.getElementById('form2js-a6_1').style.display = "table-row";
                document.getElementById('form2js-a6_3').style.display = "none";
            } else if (radio[1].checked) {
                document.getElementById('form2A6_2').checked = true
                document.getElementById('form2js-a6_1').style.display = "none";
                document.getElementById('form2js-a6_3').style.display = "none";
            } else if (radio[2].checked) {
                document.getElementById('form2A6_3').checked = true
                document.getElementById('form2js-a6_1').style.display = "none";
                document.getElementById('form2js-a6_3').style.display = "table-row";
            } else if (radio[3].checked) {
                document.getElementById('form2A6_4').checked = true
                document.getElementById('form2js-a6_1').style.display = "none";
                document.getElementById('form2js-a6_3').style.display = "none";
            }
            $('#form2A1_1').val($('#A1_1').val());
            $('#form2A1_2').val($('#A1_2').val());
            $('#form2A2_1').val($('#A2_1').val());
